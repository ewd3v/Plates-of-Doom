{
  "name": "platesOfDoom",
  "tree": {
    "$className": "DataModel",

    "ServerScriptService": {
      "Server": {
        "$path": "out/server.server.luau"
      }
    },

    "StarterPlayer": {
      "StarterPlayerScripts": {
        "Client": {
          "$path": "out/client.client.luau"
        }
      }
    },

    "Workspace": {
      "$properties": {
        "FilteringEnabled": true
      }
    },

    "Lighting": {
      "$properties": {
        "Ambient": [0, 0, 0],
        "Brightness": 2,
        "GlobalShadows": true,
        "Outlines": false,
        "Technology": "ShadowMap"
      }
    },

    "TextChatService": {
      "$properties": {
        "ChatVersion": "TextChatService"
      }
    },

    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  }
}
